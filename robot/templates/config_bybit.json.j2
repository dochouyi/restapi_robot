{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": {{ max_open_trades }},
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "dry_run": {{ dry_run | lower }},
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing":{
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "bybit",
        "key": "{{ exchange_key }}",
        "secret": "{{ exchange_secret }}",
        "ccxt_config": {
            "httpsProxy": "{{ httpsProxy }}",
            "wsProxy": "{{ httpsProxy }}",
            "enableRateLimit": true,
            "rateLimit": 50
        },
        "ccxt_async_config": {
            "enableRateLimit": true,
            "rateLimit": 50
        },
        "pair_whitelist": [
            ".*/USDT"
        ],
        "pair_blacklist": [
        ]
    },
    "pairlists": [
        {"method": "StaticPairList"}
    ],
    "api_server": {
        "enabled": true,
        "listen_ip_address": "{{ api_server_listen_addr | default("127.0.0.1", true) }}",
        "listen_port": {{ api_server_listen_port }},
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "{{ api_server_jwt_key }}",
        "ws_token": "{{ api_server_ws_token }}",
        "CORS_origins": {{ cors_origins | tojson }},
        "username": "{{ api_server_username }}",
        "password": "{{ api_server_password }}"
    },
    "bot_name": "freqtrade",
    "initial_state": "{{ initial_state }}",
    "force_entry_enable": true,
    "internals": {
        "process_throttle_secs": 5
    },
    "disable_dataframe_checks": true,
    "db_url": "sqlite:///user_data/db/tradesv3_bybit.sqlite",
    "dataformat_ohlcv": "json",
    "dataformat_trades": "jsongz"
}










